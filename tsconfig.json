{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es2019", "dom"],
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "src",
    "outDir": "dist",
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
